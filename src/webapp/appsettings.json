{
    "Logging": {
      "IncludeScopes": false,
      "Debug": {
        "LogLevel": {
          "Default": "Warning"
        }
      },
      "Console": {
        "LogLevel": {
          "Default": "Warning"
        }
      }
    },
    "SessionBackend": {
      "async": false,
      "url": "http://192.168.1.14:5001/session.json",
      "cookieName": "splicrSessionId"
    },
    "BackendSplicer": {
      "Layouts": [
        {
            "name": "standard",
            "url": "http://192.168.1.14:5001/standard.html",
            "default": true
        },
        {
            "name": "lite",
            "url": "http://192.168.1.14:5001/lite.html"
        }
      ],
      "Backends": [
        {
            "type": "regex",
            "data": {
                "hostname": "http://192.168.1.14:5001", 
                "match": "^\\/content1\\/(.*)", 
                "replace": "/$1"
            }
        },
        {
            "type": "regex",
            "data": {
                "hostname": "http://www.labaneilers.com", 
                "match": "^\\/laban\\/(.*)", 
                "replace": "/$1"
            }
        },
        {
            "type": "plugin",
            "classname": "plugin.Backend1, plugin",
            "data": {
                "hostname": "https://msdn.microsoft.com", 
                "startsegment": "/msdn"
            }
        },
        {
            "type": "regex",
            "data": {
                "hostname": "http://www.labaneilers.com", 
                "match": "(.*)", 
                "replace": "$1"
            }
        }
      ]
    }
  }
  