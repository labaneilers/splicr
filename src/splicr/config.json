    "layouts": [
        {
            "name": "standard",
            "url": "http://localhost:5001/standard.html",
            "default": true
        },
        {
            "name": "lite",
            "url": "http://localhost:5001/lite.html"
        }
    ],
    "session": {
        "async": false,
        "url": "http://localhost:5001/session.json"
    },
