{
    "backends": [
        {
            "type": "regex",
            "data": {
                "hostname": "http://localhost:5001", 
                "match": "^\\/content1\\/(.*)", 
                "replace": "/$1"
            }
        },
        {
            "type": "regex",
            "data": {
                "hostname": "http://www.labaneilers.com", 
                "match": "^\\/laban\\/(.*)", 
                "replace": "/$1"
            }
        },
        {
            "type": "plugin",
            "data": {
                "hostname": "https://msdn.microsoft.com", 
                "classname": "plugin.Backend1, plugin, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null",
                "startsegment": "/msdn"
            }
        },
