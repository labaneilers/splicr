                "replace": "/$1"
            }
        },
        {
            "type": "regex",
            "hostname": "http://www.labaneilers.com", 
            "data": {
                "match": "^\\/laban\\/(.*)", 
                "replace": "/$1"
            }
        },
        {
            "type": "regex",
            "hostname": "http://www.labaneilers.com", 
            "data": {
                "match": "(.*)", 
                "replace": "$1"
            }
        }
