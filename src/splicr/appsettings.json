{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "Plugins": [
    "../plugin/bin/Debug/netstandard2.0/"
  ],
  "SessionBackend": {
    "async": false,
    "url": "http://localhost:5001/session.json"
  },
  "BackendSplicer": {
    "Layouts": [
      {
          "name": "standard",
          "url": "http://localhost:5001/standard.html",
          "default": true
      },
      {
          "name": "lite",
          "url": "http://localhost:5001/lite.html"
      }
    ],
    "Backends": [
      {
          "type": "regex",
          "data": {
              "hostname": "http://localhost:5001", 
              "match": "^\\/content1\\/(.*)", 
              "replace": "/$1"
          }
      },
      {
          "type": "regex",
          "data": {
              "hostname": "http://www.labaneilers.com", 
              "match": "^\\/laban\\/(.*)", 
              "replace": "/$1"
          }
      },
      {
          "type": "plugin",
          "data": {
              "hostname": "https://msdn.microsoft.com", 
              "classname": "plugin.Backend1, plugin, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null",
              "startsegment": "/msdn"
          }
      },
      {
          "type": "regex",
          "data": {
              "hostname": "http://www.labaneilers.com", 
              "match": "(.*)", 
              "replace": "$1"
          }
      }
    ]
  }
}
